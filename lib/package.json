{
  "name": "lib",
  "version": "0.14.0",
  "description": "Shared code for the front-end and back-end of the Hedera Transaction Tool",
  "author": {
    "name": "Hedera",
    "url": "https://github.com/hashgraph"
  },
  "repository": "https://github.com/hashgraph/hedera-transaction-tool",
  "scripts": {
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "build": "npm run generate:database && vue-tsc --noEmit && vite build",
    "test:main": "vitest --config src/tests/vitest.config.main.ts --passWithNoTests",
    "test:main:coverage": "vitest --config src/tests/vitest.config.main.ts --coverage.enabled --coverage.include=src/main"
  },
  "dependencies": {
    "@hashgraph/sdk": "2.66.0",
    "axios": "1.8.3",
    "pinia": "2.3.1",
    "socket.io-client": "4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.19.0",
    "@types/node": "22.12.0",
    "@types/node-forge": "1.3.11",
    "@types/unzipper": "0.10.10",
    "eslint": "9.19.0",
    "prebuild-install": "7.1.3",
    "prettier": "3.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
