{
  "name": "hedera-transaction-tool",
  "version": "0.3.7",
  "description": "Transaction tool application",
  "author": {
    "name": "LimeChain",
    "url": "https://github.com/LimeChain"
  },
  "repository": "https://github.com/LimeChain/hedera-transaction-tool-release",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "vite preview",
    "dev": "vite",
    "build": "npm run  generate:database && vue-tsc --noEmit && vite build",
    "postinstall": "electron-builder install-app-deps && prisma generate",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux --config",
    "publish": "npm run build && electron-builder -m -p 'onTagOrDraft'",
    "test:main": "vitest --config src/tests/vitest.config.main.ts --passWithNoTests",
    "test:main:coverage": "vitest --config src/tests/vitest.config.main.ts --coverage.enabled --coverage.include=src/main",
    "generate:database": "prisma generate"
  },
  "dependencies": {
    "@electron-toolkit/utils": "3.0.0",
    "@hashgraph/proto": "2.15.0-beta.3",
    "@hashgraph/sdk": "2.50.0",
    "@prisma/client": "5.19.1",
    "@vuepic/vue-datepicker": "9.0.2",
    "argon2": "0.41.1",
    "axios": "1.7.7",
    "bcrypt": "5.1.1",
    "better-sqlite3": "11.2.1",
    "bignumber.js": "9.1.2",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "electron-log": "5.1.7",
    "electron-updater": "6.3.4",
    "jwt-decode": "4.0.0",
    "node-forge": "1.3.1",
    "pinia": "2.2.2",
    "socket.io-client": "4.7.5",
    "unzipper": "0.12.3",
    "vue-router": "4.4.0",
    "vue-toast-notification": "3.1.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "1.0.2",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@rushstack/eslint-patch": "1.10.4",
    "@types/bcrypt": "5.0.2",
    "@types/better-sqlite3": "7.6.11",
    "@types/bootstrap": "5.2.10",
    "@types/node": "20.14.8",
    "@types/node-forge": "1.3.11",
    "@types/unzipper": "0.10.10",
    "@vitejs/plugin-vue": "5.1.3",
    "@vitest/coverage-v8": "2.0.5",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/tsconfig": "0.5.1",
    "electron": "32.0.1",
    "electron-builder": "24.13.3",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.28.0",
    "prebuild-install": "7.1.2",
    "prettier": "3.3.3",
    "prisma": "5.19.1",
    "sass": "1.77.6",
    "typescript": "5.5.4",
    "vite": "5.4.3",
    "vite-plugin-electron": "0.28.7",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-vue-devtools": "7.4.3",
    "vitest": "2.0.5",
    "vitest-mock-extended": "2.0.2",
    "vue": "3.5.3",
    "vue-tsc": "2.1.6"
  }
}
