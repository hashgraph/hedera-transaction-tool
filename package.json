{
  "name": "hedera-transaction-tool",
  "version": "0.2.6-beta.1",
  "description": "Transaction tool application",
  "author": {
    "name": "LimeChain",
    "url": "https://github.com/LimeChain"
  },
  "repository": "https://github.com/LimeChain/hedera-transaction-tool-release",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "vite preview",
    "devTools": "./node_modules/.bin/vue-devtools",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux --config",
    "publish": "npm run build && electron-builder -m -p 'onTagOrDraft'",
    "test:main": "vitest --config src/tests/vitest.config.main.ts --passWithNoTests",
    "test:renderer": "vitest --config src/tests/vitest.config.renderer.ts --passWithNoTests",
    "test:e2e": "npm run build && vitest --config src/tests/vitest.config.e2e.ts --passWithNoTests",
    "test:main:coverage": "vitest --config src/tests/vitest.config.main.ts --coverage.enabled --coverage.include=src/main",
    "test:renderer:coverage": "vitest --config src/tests/vitest.config.renderer.ts --coverage.enabled --coverage.include=src/renderer"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "@hashgraph/sdk": "^2.42.0-beta.4",
    "@prisma/client": "^5.10.2",
    "axios": "^1.6.7",
    "better-sqlite3": "^9.4.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "electron-log": "^5.1.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.8",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.1.7",
    "vue-router": "^4.3.0",
    "vue-toast-notification": "^3.1.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/better-sqlite3": "^7.6.9",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/devtools": "^6.5.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "electron": "28.2.5",
    "electron-builder": "^24.13.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "happy-dom": "^13.6.2",
    "less": "^4.2.0",
    "playwright": "^1.42.0",
    "prettier": "3.2.5",
    "prisma": "^5.10.2",
    "sass": "^1.71.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-electron": "^0.28.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.3.1",
    "vue": "^3.4.21",
    "vue-tsc": "^1.8.27"
  }
}
