{
  "name": "hedera-transaction-tool",
  "version": "0.1.15",
  "description": "Transaction tool application",
  "main": "build/main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "devTools": "./node_modules/.bin/vue-devtools",
    "lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build": "node scripts/build.js && electron-builder",
    "build:win": "node scripts/build.js && electron-builder --win",
    "build:mac": "node scripts/build.js && electron-builder --mac",
    "build:linux": "node scripts/build.js && electron-builder --linux",
    "publish": "node scripts/build.js && electron-builder -m -p 'onTagOrDraft'"
  },
  "repository": "https://github.com/LimeChain/hedera-transaction-tool-release",
  "author": {
    "name": "LimeChain",
    "url": "https://github.com/LimeChain"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/devtools": "^7.0.5",
    "@vue/eslint-config-typescript": "^12.0.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "sass": "^1.69.7",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@hashgraph/sdk": "^2.39.0",
    "@types/bootstrap": "^5.2.10",
    "axios": "^1.6.4",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "dotenv": "^16.3.1",
    "electron-log": "^5.0.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5",
    "vue-toast-notification": "^3.1.2"
  }
}
