{
  "name": "hedera-transaction-tool",
  "version": "0.2.5-beta.1",
  "description": "Transaction tool application",
  "author": {
    "name": "LimeChain",
    "url": "https://github.com/LimeChain"
  },
  "repository": "https://github.com/LimeChain/hedera-transaction-tool-release",
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "devTools": "./node_modules/.bin/vue-devtools",
    "dev": "electron-vite dev -w",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux --config",
    "publish": "npm run build && electron-builder -m -p 'onTagOrDraft'"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "@hashgraph/proto": "^2.14.0-beta.3",
    "@hashgraph/sdk": "^2.40.0",
    "@prisma/client": "^5.8.1",
    "@types/bootstrap": "^5.2.10",
    "axios": "^1.6.5",
    "better-sqlite3": "^9.3.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "electron-log": "^5.0.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.1.7",
    "vue-router": "^4.2.5",
    "vue-toast-notification": "^3.1.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.7.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/devtools": "^6.5.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "electron": "28.1.1",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "less": "^4.2.0",
    "prettier": "3.1.1",
    "prisma": "^5.8.1",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}
