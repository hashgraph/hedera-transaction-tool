    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "create-admin": "npx ts-node ./scripts/create-admin.ts",
    "create-users": "npx ts-node ./scripts/add-users.ts"
  },
